{
  "name": "screenshot-to-code",
  "displayName": "Screenshot to React+Tailwind",
  "description": "Convert screenshots into React components with Tailwind CSS",
  "version": "0.1.0",
  "publisher": "screenshot-to-code",
  "repository": {
    "type": "git",
    "url": "https://github.com/abi/screenshot-to-code"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:screenshot-to-code.generateComponent"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "screenshot-to-code.generateComponent",
        "title": "Generate React Component from Screenshot"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname =~ /\\.(png|jpg|jpeg|gif)$/",
          "command": "screenshot-to-code.generateComponent",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "tailwind",
    "screenshot",
    "component",
    "generator"
  ]
}
